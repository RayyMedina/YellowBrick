ALTER proc [dbo].[ClientEmployment_DeleteById]
				( @Id int )

AS
/*

		Declare @Id int = 111;


		Execute dbo.ClientEmployment_DeleteById

				@Id 



SELECT *
FROM dbo.ClientEmployment

*/
BEGIN

		DELETE FROM dbo.ClientEmployment
		WHERE Id = @Id



END


