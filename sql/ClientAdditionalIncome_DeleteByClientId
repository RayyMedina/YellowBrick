ALTER proc [dbo].[ClientAdditionalIncome_DeleteByClientId]
				( @ClientId int )

AS
/*

		Declare @ClientId int = 6;


		Execute dbo.ClientAdditionalIncome_DeleteByClientId

				@ClientId



SELECT *
FROM dbo.ClientAdditionalIncome

*/
BEGIN

		DELETE FROM dbo.ClientAdditionalIncome
		WHERE ClientId = @ClientId



END


